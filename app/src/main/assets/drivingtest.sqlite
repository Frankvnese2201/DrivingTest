INSERT INTO DrivingTest (_id, Question, Answer1, Answer2, Answer3, Answer4, Result, Score, TypeLicense, Image) VALUES (1, 'Khái ni?m “phuong ti?n giao thông thô so du?ng b?” du?c hi?u th? nào là dúng?', 'G?m xe d?p (k? c? xe d?p máy, xe d?p di?n), xe xích lô, xe lan dùng cho ngu?i khuy?t t?t, xe súc v?t kéo và các lo?i xe tuong t?.', 'G?m xe d?p (k? c? xe d?p máy, xe d?p di?n), xe g?n máy, xe co gi?i dùng cho ngu?i khuy?t t?t và xe máy chuyên dùng.', 'G?m xe ô tô, máy kéo, ro moóc ho?c so mi ro moóc du?c kéo b?i xe ô tô, máy kéo', NULL, '1', NULL, 'b2', NULL);
INSERT INTO DrivingTest (_id, Question, Answer1, Answer2, Answer3, Answer4, Result, Score, TypeLicense, Image) VALUES (2, '“Phuong ti?n tham gia giao thông du?ng b?” g?m nh?ng lo?i nào?', 'Phuong ti?n giao thông co gi?i du?ng b?.', 'Phuong ti?n giao thông thô so du?ng b? và xe máy chuyên dùng.', 'C? ý 1 và ý 2', NULL, '3', NULL, 'b2', NULL);
INSERT INTO DrivingTest (_id, Question, Answer1, Answer2, Answer3, Answer4, Result, Score, TypeLicense, Image) VALUES (3, 'Trên du?ng có nhi?u làn du?ng cho xe di cùng chi?u du?c phân bi?t b?ng v?ch k? phân làn du?ng, ngu?i di?u khi?n phuong ti?n ph?i cho xe di nhu th? nào?', 'Cho xe di trên b?t k? làn du?ng nào ho?c gi?a 02 làn du?ng n?u không có xe phía tru?c; khi c?n thi?t ph?i chuy?n làn du?ng, ngu?i lái xe ph?i quan sát xe phía tru?c d? b?o d?m an toàn.', 'Cho xe di trên b?t k? làn du?ng nào ho?c gi?a 02 làn du?ng n?u không có xe phía tru?c; khi c?n thi?t ph?i chuy?n làn du?ng, ngu?i lái xe ph?i quan sát xe phía tru?c d? b?o d?m an toàn.Ph?i cho xe di trong m?t làn du?ng và ch? du?c chuy?n làn du?ng ? nh?ng noi cho phép; khi chuy?n làn ph?i có tín hi?u báo tru?c và ph?i b?o d?m an toàn.', 'Ph?i cho xe di trong m?t làn du?ng, khi c?n thi?t ph?i chuy?n làn du?ng, ngu?i lái xe ph?i quan sát xe phía tru?c d? b?o d?m an toàn.', NULL, '2', NULL, 'b2', NULL);
INSERT INTO DrivingTest (_id, Question, Answer1, Answer2, Answer3, Answer4, Result, Score, TypeLicense, Image) VALUES (4, 'Khi tránh xe di ngu?c chi?u, các xe ph?i nhu?ng du?ng nhu th? nào là dúng quy t?c giao thông?', 'Noi du?ng h?p ch? d? cho m?t xe ch?y và có ch? tránh xe thì xe nào ? g?n ch? tránh hon ph?i vào v? trí tránh, nhu?ng du?ng cho xe kia di.', 'Xe xu?ng d?c ph?i nhu?ng du?ng cho xe dang lên d?c; xe nào có chu?ng ng?i v?t phía tru?c ph?i nhu?ng du?ng cho xe không có chu?ng ng?i v?t di tru?c.', 'Xe lên d?c ph?i nhu?ng du?ng cho xe xu?ng d?c; xe nào không có chu?ng ng?i v?t phía tru?c ph?i nhu?ng du?ng cho xe có chu?ng ng?i v?t di tru?c', 'C? ý 1 và ý 2.', '4', NULL, 'b2', NULL);
INSERT INTO DrivingTest (_id, Question, Answer1, Answer2, Answer3, Answer4, Result, Score, TypeLicense, Image) VALUES (5, 'T?i noi du?ng b? giao nhau cùng m?c v?i du?ng s?t ch? có dèn tín hi?u ho?c chuông báo hi?u, khi dèn tín hi?u màu d? dã b?t sáng ho?c có ti?ng chuông báo hi?u, ngu?i tham gia giao thông ph?i d?ng l?i ngay và gi? kho?ng cách t?i thi?u bao nhiêu mét tính t? ray g?n nh?t?', '5 mét.', '3 mét.', '4 mét.', NULL, '1', NULL, 'b2', NULL);
INSERT INTO DrivingTest (_id, Question, Answer1, Answer2, Answer3, Answer4, Result, Score, TypeLicense, Image) VALUES (6, 'Ngu?i di?u khi?n phuong ti?n tham gia giao thông trong h?m du?ng b? ngoài vi?c ph?i tuân th? các quy t?c giao thông còn ph?i th?c hi?n nh?ng quy d?nh nào du?i dây?', 'Xe co gi?i, xe máy chuyên dùng ph?i b?t dèn; xe thô so ph?i b?t dèn ho?c có v?t phát sáng báo hi?u; ch? du?c d?ng xe, d? xe ? noi quy d?nh.', 'Xe co gi?i ph?i b?t dèn ngay c? khi du?ng h?m sáng; ph?i cho xe ch?y trên m?t làn du?ng và ch? chuy?n làn ? noi du?c phép; du?c quay d?u xe, lùi xe khi c?n thi?t.', 'Xe máy chuyên dùng ph?i b?t dèn ngay c? khi du?ng h?m sáng; ph?i cho xe ch?y trên m?t làn du?ng và ch? chuy?n làn ? noi du?c phép; du?c quay d?u xe, lùi xe khi c?n thi?t.', NULL, '1', NULL, 'b2', NULL);
INSERT INTO DrivingTest (_id, Question, Answer1, Answer2, Answer3, Answer4, Result, Score, TypeLicense, Image) VALUES (7, 'Trên do?n du?ng hai chi?u không có gi?i phân cách gi?a, ngu?i lái xe không du?c vu?t xe khác trong các tru?ng h?p nào du?i dây?', 'Xe b? vu?t b?t ng? tang t?c d? và c? tình không nhu?ng du?ng.', 'Xe b? vu?t gi?m t?c d? và nhu?ng du?ng.', 'Phát hi?n có xe di ngu?c chi?u.', 'C? ý 1 và ý 3.', '4', NULL, 'b2', NULL);
INSERT INTO DrivingTest (_id, Question, Answer1, Answer2, Answer3, Answer4, Result, Score, TypeLicense, Image) VALUES (8, 'Trên du?ng b? ngoài khu v?c dông dân cu, du?ng hai chi?u không có d?i phân cách gi?a; du?ng m?t chi?u có m?t làn xe co gi?i (tr? du?ng cao t?c), lo?i xe nào tham gia giao thông v?i t?c d? t?i da cho phép là 60 km/h?', 'Ô tô kéo ro moóc, ô tô kéo xe khác, xe g?n máy.', 'Ô tô ch? ngu?i trên 30 ch? (tr? ô tô buýt), ô tô t?i có tr?ng t?i trên 3.500 kg.', 'Ô tô buýt, ô tô d?u kéo kéo so mi ro moóc, xe mô tô, ô tô chuyên dùng (tr? ô tô tr?n v?a, ô tô tr?n bê tông).', NULL, '4', NULL, 'b2', NULL);
